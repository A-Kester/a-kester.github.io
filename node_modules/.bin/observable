#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/dist/bin/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/dist/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/dist/bin/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/dist/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/framework/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules/@observablehq/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/@observablehq+framework@1.13.3_@types+markdown-it@14.1.2/node_modules:/Users/annakester/a-kester.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@observablehq/framework/dist/bin/observable.js" "$@"
else
  exec node  "$basedir/../@observablehq/framework/dist/bin/observable.js" "$@"
fi
